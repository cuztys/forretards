local remotes = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Functions")
local joinRole = remotes:WaitForChild("JoinRole")

for i = 1, 99999 do
    task.spawn(function()
        local args = {
            [1] = "Guard1x",
            [2] = true
        }
        joinRole:InvokeServer(unpack(args))
    end)
end
